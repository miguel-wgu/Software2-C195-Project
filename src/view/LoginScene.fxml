<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="375.0" prefWidth="488.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
    <children>
        <TextField fx:id="userNameTextField" layoutX="170.0" layoutY="101.0" promptText="Username" />
        <PasswordField fx:id="loginPasswordField" layoutX="170.0" layoutY="148.0" promptText="Password" />
        <Button fx:id="loginBtn" layoutX="196.0" layoutY="187.0" mnemonicParsing="false" onAction="#loginBtnClick" prefHeight="25.0" prefWidth="98.0" text="Login" />
        <Button fx:id="clearLoginBtn" layoutX="195.0" layoutY="227.0" mnemonicParsing="false" onAction="#clearLoginBtnClick" prefHeight="25.0" prefWidth="98.0" text="Clear" />
        <Label fx:id="schedulerLoginLabel" alignment="CENTER" layoutX="-1.0" layoutY="44.0" prefHeight="35.0" prefWidth="488.0" text="Appointment Scheduler Login">
            <font>
                <Font name="Nirmala UI" size="24.0" />
            </font></Label>
        <Label fx:id="currentLocationLabel" alignment="CENTER" layoutX="-1.0" layoutY="276.0" prefHeight="27.0" prefWidth="488.0" text="Current Location">
            <font>
                <Font name="Nirmala UI" size="18.0" />
            </font>
        </Label>
        <Label fx:id="timezoneLabel" alignment="CENTER" layoutX="-1.0" layoutY="316.0" prefHeight="27.0" prefWidth="488.0" text="Placeholder" textAlignment="CENTER">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
